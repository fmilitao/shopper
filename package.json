{
  "name": "shopper",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://fmilitao.github.io/shopper/",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.3.6",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "ajv": "^6.12.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-toastify": "^6.0.5",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "yarn run buildVersion",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --verbose=true --setupFilesAfterEnv=./src/shopper/__test__/setupTests.ts",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "buildVersion": "echo \"export default 'commit: `git rev-parse --short HEAD` (`node -p 'new Date().toISOString()'`)';\" > src/version.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.0.0"
  }
}
